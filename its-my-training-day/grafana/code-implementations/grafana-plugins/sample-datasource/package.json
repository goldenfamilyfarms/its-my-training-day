{
  "name": "sample-datasource",
  "version": "1.0.0",
  "description": "A sample Grafana data source plugin for interview preparation",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "e2e": "playwright test",
    "server": "docker-compose up --build"
  },
  "author": "Grafana Interview Prep",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^7.0.0",
    "@grafana/plugin-e2e": "^1.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@playwright/test": "^1.41.0",
    "@swc/core": "^1.3.100",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@grafana/data": "^10.3.0",
    "@grafana/runtime": "^10.3.0",
    "@grafana/ui": "^10.3.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.21"
}
